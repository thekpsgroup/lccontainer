  description="Professional container delivery service across Texas. We deliver to construction sites, businesses, and residential locations with our specialized equipment and experienced team."
>
  <Header />
  
  <!-- Hero Section -->
  <section class="relative bg-gradient-to-br from-primary-600 to-primary-700 text-white py-20">
    <div class="absolute inset-0 bg-black/20"></div>
    <div class="container mx-auto px-4 relative z-10">
      <div class="max-w-4xl mx-auto text-center">
        <h1 class="text-5xl md:text-6xl font-bold mb-6 animate-fade-in">
          Container Delivery
        </h1>
        <p class="text-xl md:text-2xl mb-8 animate-slide-up">
          Professional Delivery Service Across Texas
        </p>
        <div class="bg-white/10 backdrop-blur-sm rounded-2xl p-8 max-w-2xl mx-auto">
          <div class="text-4xl mb-4">ðŸš›</div>
          <p class="text-lg mb-4">
            From our yard to your location with precision and care
          </p>
          <div class="flex flex-wrap justify-center gap-4 text-sm">
            <div class="bg-white/20 rounded-lg px-3 py-1">âœ“ Statewide Service</div>
            <div class="bg-white/20 rounded-lg px-3 py-1">âœ“ Specialized Equipment</div>
            <div class="bg-white/20 rounded-lg px-3 py-1">âœ“ Experienced Team</div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Delivery Services -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4">
